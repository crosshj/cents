{
  "name": "nodecents",
  "version": "0.0.0",
  "private": true,
  "description": "Personal finance done with node.js.  Some DropBox integration.  WIP.",
  "main": "node cents.node.server.js",
  "dependencies": {
    "async": "^2.6.0",
    "body-parser": "^1.16.0",
    "colog": "^1.0.4",
    "compression": "1.7.4",
    "connect-mongodb-session": "^2.2.0",
    "connect-redis": "3.3.3",
    "cookie-parser": "^1.4.4",
    "cron": "^1.2.1",
    "diff-json": "2.0.0",
    "express": "4.16.2",
    "express-session": "1.15.6",
    "http-proxy": "^1.12.0",
    "moment": "^2.17.1",
    "passport": "0.3.2",
    "passport-local": "1.0.0",
    "request": "^2.87.0",
    "serve-static": "^1.14.1",
    "session-file-store": "1.3.1",
    "simple-object-path": "1.1.0",
    "sync-request": "^2.0.1",
    "tingodb": "0.6.1",
    "xml2js": "^0.4.4"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "10.0.3",
    "babel-jest": "25.0.0",
    "babel-loader": "8.0.0",
    "copy-webpack-plugin": "5.1.1",
    "css-loader": "1.0.0",
    "dayjs": "1.8.15",
    "eslint": "6.3.0",
    "eslint-plugin-react": "7.14.3",
    "eslint-watch": "6.0.0",
    "file-loader": "1.1.6",
    "flickity": "2.0.5",
    "highcharts": "9.0.0",
    "jest": "24.9.0",
    "jest-junit": "8.0.0",
    "jquery": "^3.2.1",
    "lodash": "^4.12.0",
    "mini-css-extract-plugin": "0.8.0",
    "nodemon": "1.18.3",
    "preact": "8.5.2",
    "preact-compat": "3.19.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-flickity-component": "git+https://github.com/crosshj/react-flickity-component.git",
    "react-highcharts": "16.0.2",
    "react-mfb": "^0.6.0",
    "react-sidebar": "3.0.2",
    "react-test-renderer": "16.9.0",
    "redux": "^3.7.2",
    "sass": "1.22.10",
    "sass-loader": "8.0.0",
    "style-loader": "0.21.0",
    "webpack": "4.39.2",
    "webpack-cli": "3.3.6",
    "webpack-dev-middleware": "3.7.0",
    "webpack-livereload-plugin": "^2.2.0",
    "webpack-node-externals": "1.7.2"
  },
  "jest": {
    "verbose": true,
    "collectCoverageFrom": [
      "server/**/*.{js,jsx}",
      "client/components/*.{js,jsx}",
      "client/reducers/*.{js,jsx}"
    ],
    "coverageDirectory": "./build/coverage/",
    "coverageReporters": [
      "text",
      "cobertura"
    ],
    "reporters": [
      "default",
      "jest-junit"
    ],
    "setupFiles": [
      "./__test-helpers__/helpers.js"
    ]
  },
  "jest-junit": {
    "outputDirectory": "./build/reports/",
    "outputName": "jest-junit.xml"
  },
  "browserslist": [
    "last 1 version",
    "> 1%",
    "maintained node versions",
    "not dead"
  ],
  "scripts": {
    "install": "./service/init.sh",
    "build": "webpack",
    "clean": "npm prune --production",
    "start": "node dist/server",
    "watch": "nodemon server/devIndex.js",
    "prestart": "touch ./accounts.json",
    "bundle.stats": "webpack --profile --json > webpack-stats.json && echo https://chrisbateman.github.io/webpack-visualizer/",
    "lint": "eslint . --ext .js,.jsx",
    "lint.watch": "esw . --ext .js,.jsx --watch --color",
    "build.deprecate": "browserify index.js > myproject.min.js",
    "test": "jest",
    "test.ci": "jest --ci --coverage",
    "test.watch": "npm test -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/crosshj/nodeCents.git"
  },
  "keywords": [
    "finance"
  ],
  "author": "HJ Cross",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/crosshj/nodeCents/issues"
  },
  "homepage": "https://github.com/crosshj/nodeCents"
}
